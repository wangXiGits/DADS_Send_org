.navbar-inner {
    color: #000000;
    filter: progid:DXImageTransform.Microsoft.gradient(gradientType=0, startcolorstr=#8ccaee, endcolorstr=#f2f2f2);
    background: -ms-linear-gradient(top, #8ccaee, #f2f2f2);
    background: -moz-linear-gradient(top, #8ccaee, #f2f2f2);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#8ccaee), to(#f2f2f2));
    background: -webkit-linear-gradient(top, #8ccaee, #f2f2f2);
    background: -o-linear-gradient(top, #8ccaee, #f2f2f2);
    background: linear-gradient(top, #8ccaee, #f2f2f2);
}

.navbar .brand {
    padding: 8px 20px 12px;
    font-size: 20px;
    color: #727272;
}

.navbar .divider-vertical {
    height: 41px;
    background-color: #ffffff;
    border-right: 1px solid #c6c6c6;
}

.navbar .nav > li > a {
    padding: 10px 10px 11px;
    font-size: 16px;
    color: #666666;
}

.navbar .nav > .active > a,
.navbar .nav > .active > a:hover,
.navbar .nav > .active > a:focus {
    background-color: #fb5640;
    color: #ffffff;
}